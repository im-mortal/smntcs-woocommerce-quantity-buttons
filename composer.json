{
	"name": "nielslange/smntcs-woocommerce-quantity-buttons",
	"type": "plugin",
	"description": "Display quantity buttons on WooCommerce product page",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Private Site"
	],
	"homepage": "https://github.com/nielslange/smntcs-woocommerce-quantity-buttons",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Contributors",
			"homepage": "https://github.com/nielslange/smntcs-woocommerce-quantity-buttons/graphs/contributors"
		}
	],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
		"php-parallel-lint/php-parallel-lint": "^1.2.0",
		"wptrt/wpthemereview": "^0.2.1"
	},
	"scripts": {
		"checkcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs --runtime-set testVersion 5.6-",
		"fixcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf --runtime-set testVersion 5.6-",
		"lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
	},
	"support": {
		"issues": "https://github.com/nielslange/smntcs-woocommerce-quantity-buttons/issues",
		"source": "https://github.com/nielslange/smntcs-woocommerce-quantity-buttons"
	}
}
